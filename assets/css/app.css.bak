@import 'https://s3.amazonaws.com/eseb2013/css/select2/select2.8e4d0297a17ac45fe7e812fbb68c8680.css';
html{height:100%;background:url("/img/bg.png") repeat fixed;-webkit-background-size:auto,cover,cover;-moz-background-size:auto,cover,cover;background-size:auto,cover,cover}
body{height:100%;font-family:"Droid Sans","Helvetica Neue",Helvetica,Arial,sans-serif;color:#333;background-color:transparent}
p{font-family:'Droid Sans','Helvetica Neue',Helvetica,Arial,sans-serif}
p.big{font-size:16pt;line-height:1.5;}
p.big span.label.label-important{font-size:14pt}
h1{text-align:center}
.page-header{border-bottom:1px solid #ddd}
.container-fluid .content{background:#fff;background:-webkit-gradient(linear, left top, left bottom, color-stop(0, #fff), color-stop(0.47, #f6f6f6), color-stop(1, #ededed));background:-webkit-linear-gradient(top, #fff 0%, #f6f6f6 47%, #ededed 100%);background:-moz-linear-gradient(top, #fff 0%, #f6f6f6 47%, #ededed 100%);background:-o-linear-gradient(top, #fff 0%, #f6f6f6 47%, #ededed 100%);background:-ms-linear-gradient(top, #fff 0%, #f6f6f6 47%, #ededed 100%);background:linear-gradient(top, #fff 0%, #f6f6f6 47%, #ededed 100%);border:2px solid #999;-webkit-border-bottom-left-radius:14px;border-bottom-left-radius:14px;-webkit-border-bottom-right-radius:14px;border-bottom-right-radius:14px;-webkit-box-shadow:0 5px 5px #000;box-shadow:0 5px 5px #000;margin-bottom:10px;padding:20px;padding-top: 100px;display: inline-block;}
article{text-shadow:1px 1px 0 #fff}
.nav-pills{text-shadow:none}
.carousel-inner{text-shadow:none}
article aside{margin-left:0;padding-left:19px;border-left:1px solid #eee;text-align:center}
footer{border-top:1px solid #eee;padding-top:5px;padding-bottom:0;font-size:.8em;text-align:center}
.centered{text-align:center}
.navbar .nav > li > a{line-height:30px;padding:10px 10px 10px}
.navbar.navbar-fixed-top.navbar-inverse{font-family:'Ubuntu','Helvetica Neue',Helvetica,Arial,sans-serif}
.carousel-inner{text-align:center}
.carousel .item > img{width:100%}
.thumbnail > .loading > img{height:150px}
.thumbnails{text-align:center}
.thumbnails > li{display:inline-block;float:none}
.nav-collapse .nav > li > a.build,.nav-collapse .dropdown-menu a.build{color:#666}
table.schedule{opacity:.9;-ms-filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=90);filter:alpha(opacity=90);width:auto}
table.schedule th{text-align:center;vertical-align:middle;border:1px solid #fff}
table.schedule td{line-height:90%;max-width:90px;border:1px solid #fff;text-align:center;vertical-align:middle}
table.schedule td.rowname{font-weight:900}
table.schedule .even{color:#fff;background-color:#999;text-shadow:-1px -1px 0 #777}
table.schedule .odd{color:#fff;background-color:#999;text-shadow:-1px -1px 0 #777}
table.schedule .registration,table.schedule .tour{background-color:#cdcdcd;color:#000}
table.schedule .others{background-color:#f1d9ad;color:#ee1e00}
table.schedule .empty{border:none}
table.schedule .plenary{background-color:#efc5c3;color:#000}
table.schedule .coffee,table.schedule .lunch{background-color:#e6e6e6;color:#000}
table.schedule .parallel{background-color:#b9c88b;color:#000}
table.schedule .posters{background-color:#87aabb;color:#000}
table.schedule .welcome{background-color:#c0504e;color:#fff;text-shadow:-1px -1px 0 #777}
table.schedule .dinner{background-color:#c0504e;color:#fff;text-shadow:-1px -1px 0 #777}
#pageslide{display:none;position:absolute;position:fixed;top:0;height:100%;z-index:999999;width:260px;padding:20px;background-color:#1b1b1b;color:#fff;-webkit-box-shadow:inset 0 0 5px 5px #222;box-shadow:inset 0 0 5px 5px #222;overflow:auto}
#contact{display:none;padding-bottom:30px}
img.logoimg{max-width:none;padding-right:5px}
img.navpic{height:40px;vertical-align:middle}
img.broken{-webkit-transform:rotate(7deg);-moz-transform:rotate(7deg);-o-transform:rotate(7deg);-ms-transform:rotate(7deg);transform:rotate(7deg)}
#logotext{color:rgba(240,240,240,0.85);text-shadow:0 -1px 0 rgba(0,0,0,0.2);font-weight:400;height:30px;display:inline-block;vertical-align:middle;font-size:.7em;line-height:1.1em;-webkit-text-size-adjust:none;-moz-text-size-adjust:none;font-smooth:always;-webkit-font-smoothing:antialiased;zoom:1;*display:inline}
#logotext .serif{font-family:"Droid Serif",Constantia,"Lucida Bright",Lucidabright,"Lucida Serif",Lucida,"DejaVu Serif","Bitstream Vera Serif","Liberation Serif",Georgia,serif}
#logotext .smalltext{font-size:.7em}
#logotext .bigtext{font-size:1.2em;font-weight:400}
.navbar-form .input-append .btn,.navbar-form .input-prepend .btn{margin-top:0}
.navbar-form .btn,.navbar-form .btn-group .appendedbtn{padding:4px}
.navbar-form input{margin-top:6px}
#user-panel{padding:5px}
#login-field{border:2px;-webkit-transition:all 1s ease-in-out;-moz-transition:all 1s ease-in-out;-o-transition:all 1s ease-in-out;-ms-transition:all 1s ease-in-out;transition:all 1s ease-in-out}
#login-field{float:right;padding-top:5px;padding-right:5px}
#login-field > .login-field-elt{float:left;display:inline-block;margin-right:5px}
#login-field.flash_highlight{opacity:.7;-ms-filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=70);filter:alpha(opacity=70);-webkit-transition:all 1s ease-in-out;-moz-transition:all 1s ease-in-out;-o-transition:all 1s ease-in-out;-ms-transition:all 1s ease-in-out;transition:all 1s ease-in-out}
#nav-username{color:#ddd;margin:10px;font-size:2em;font-weight:normal;line-height:30px;padding-top:10px;vertical-align:middle}
th.headerSortUp:after{font-family:'FontAwesomeMod';content:'\f0d8'}
th.headerSortDown:after{font-family:'FontAwesomeMod';content:'\f0d7'}
th.header{color:#08c}
.affiliation-fields .control-group{margin-bottom:5px}
#firstaffiliation .control-group{margin-bottom:5px}
td > button.btn.up,td.sort > button.btn.down{color:#08c}
td > button.btn.remove{color:#b94a48}
td.abstract > a.btn{margin:5px;float:right;display:block}
li.abstract{line-height:40px}
#coauthors.table td{vertical-align:middle}
.controls > a#abstractgoback{margin-left:5px}
.bigdrop.select2-container .select2-results{max-height:300px}
.bigdrop .select2-results{max-height:300px}
.select2-result-selectable .select2-match,.select2-result-unselectable .select2-result-selectable .select2-match{text-decoration:underline;background-color:#b58900}
.symposium-image{width:40px;padding:5px;vertical-align:text-top;}
.symposium-image img{width:40px}
.symposium-title{font-weight:700}
li.select2-results-dept-0.select2-result.select2-result-selectable.select2-highlighted{background-color:#6495ed}
.table-framed{border:1px solid #ddd}
td.rowname{font-weight:900}
.navbar-fixed-top .container,.navbar-fixed-bottom .container{width:auto;padding-left:10px;margin-bottom:0}
#account{width:4%}
.account-select{width:auto;}
.account-select i{font-size:22px}
.account-select-dropdown{min-width:135px;-webkit-transform:translate(-60%,2%);-moz-transform:translate(-60%,2%);-o-transform:translate(-60%,2%);-ms-transform:translate(-60%,2%);transform:translate(-60%,2%);color:#444;-webkit-border-radius:3px 3px 3px 3px;border-radius:3px 3px 3px 3px;}
.account-select-dropdown span{vertical-align:top}
.account-select-dropdown i{font-size:22px}
.icon-eseb2013-sign:before{content:"\f0eb"}
.icon-google-sign:before{content:"\f0d4"}
#forgot-form > button{float:left;margin-top:10px;margin-bottom:10px}
.media img{margin-right:10px;width:200px}
#registrations_placeholder{color:#777;text-decoration:none;vertical-align:middle;padding-top:15px;padding-right:15px;font-size:1.5em}
#registrations_placeholder:hover{color:#eee}
.controls.noinput{padding-top:5px}
.popover.italics{width:auto}
.loading{background:url("https://s3.amazonaws.com/eseb2013/img/style/loading.eb4bfcf1a319f71cb092cc4b74e5be0e.gif") no-repeat center}
.fadein{opacity:0;-ms-filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=0);filter:alpha(opacity=0);-webkit-transition:opacity 2s;-moz-transition:opacity 2s;-o-transition:opacity 2s;-ms-transition:opacity 2s;transition:opacity 2s}
@media (min-width: 980px){.container-fluid{width:980px;margin:0 auto}
}@media (max-width: 1280px){.navbar-fixed-top{margin-bottom:0}
}@media (max-width: 1324px){#logotext{display:none}
}@media (max-width: 375px){#logotext{display:none}
}@media (max-width: 800px){html{background:#e5b480;background-image:url("../img/bg800x631.jpg");background-repeat:no-repeat;-webkit-background-size:100%;-moz-background-size:100%;background-size:100%;background-position:center center}
.loading{background-image:none}
}